{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "astralgalaxymusicplayer",
  "version": "0.1.0",
  "identifier": "top.maple-bamboo.astralgalaxy.musicplayer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "astralgalaxymusicplayer",
        "width": 1313,
        "height": 766,
        "center": true,
        "decorations": false,
        "transparent": true,
        "resizable": true,
        "minWidth": 900,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
